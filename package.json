{
  "name": "timelink-snippets-ilhavo",
  "displayName": "timelink-snippets-ilhavo",
  "description": "Snippets extension with Parish registers (actos paroquiais) for usage with Time Link. Time Link (formerly MHK / TimeLink) is a web application that manages databases for micro-historical research.",
  "version": "1.0.0",
  "publisher": "time-link",
  "engines": {
    "vscode": "^1.70.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/time-link/timelink-snippets-ilhavo.git"
  },
  "categories": [
    "Snippets"
  ],
  "contributes": {
    "snippets": [
      {
        "language": "kleio",
        "path": "./snippets/snippets.code-snippets"
      }
    ],
    "languages": [
      {
        "id": "kleio",
        "aliases": [
          "Kleio",
          "kleio"
        ],
        "extensions": [
          ".cli",
          ".kleio",
          ".rpt",
          ".CLI",
          ".KLEIO",
          ".org",
          ".old"
        ],
        "configuration": "./language-configuration.json"
      }
    ]
  }
}